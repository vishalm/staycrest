{
  "name": "staycrest",
  "version": "1.0.0",
  "description": "AI-Powered Hotel Discovery & Loyalty Aggregation Platform",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "dev:docker": "docker-compose -f docker-compose-dev.yml up",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:client": "jest tests/client",
    "test:server": "jest tests/server",
    "test:v1": "jest server/test/v1",
    "test:worker": "jest tests/server/worker.test.js tests/server/worker-threads.test.js",
    "test:cluster": "jest tests/server/cluster.test.js",
    "test:middleware": "jest tests/server/middleware.test.js",
    "test:redis": "jest tests/server/redis-connection.test.js",
    "test:tracing": "jest tests/server/tracing.test.js",
    "test:health": "jest tests/server/health.test.js",
    "test:infrastructure": "jest tests/server/worker.test.js tests/server/worker-threads.test.js tests/server/cluster.test.js tests/server/middleware.test.js tests/server/redis-connection.test.js tests/server/tracing.test.js tests/server/health.test.js",
    "lint": "eslint .",
    "build": "webpack --mode production",
    "db:init": "node server/database/init_db.js",
    "db:migrate:up": "node server/database/migrate.js up",
    "db:migrate:down": "node server/database/migrate.js down",
    "seed:hotel-data": "node server/database/seeds/hotel_data_seed.js",
    "swagger:generate": "node ./scripts/generate-swagger.js",
    "cluster": "node cluster.js",
    "metrics": "node server/metrics/collect-metrics.js",
    "prometheus": "node server/metrics/prometheus-exporter.js",
    "analyze": "node server/metrics/analyze-performance.js",
    "simple-server": "node server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/staycrest.git"
  },
  "keywords": [
    "ai",
